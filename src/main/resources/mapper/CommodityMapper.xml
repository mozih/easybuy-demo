<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mo.easybuy.mapper.CommodityMapper">

    <update id="addComScan" parameterType="Integer">
        update commodity set comScan = comScan + 1 where comId = #{comId}
    </update>
</mapper>
